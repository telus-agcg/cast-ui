{
  "name": "@tkxs/cast-ui",
  "version": "0.1.8",
  "author": "@tkxs",
  "description": "React component library for the TKXS design system",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/technekes/cast-ui"
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config webpack.prod.js",
    "build-dev": "npm run clean && tsc -w -m es6 --outDir lib-esm",
    "storybook": "start-storybook -p 6006 --ci -c .storybook",
    "lint": "tslint --project tsconfig.json",
    "clean": "shx rm -rf lib lib-esm lib-umd",
    "deploy-storybook": "storybook-to-ghpages --ci",
    "build-and-publish": "npm run build && npm publish"
  },
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "peerDependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "styled-components": "^4.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react-modal": "^3.8.1",
    "react-select": "^2.2.0",
    "react-table": "^6.8.6",
    "react-tabs": "^2.3.0",
    "react-through": "^1.1.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@storybook/addon-actions": "^4.0.6",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-viewport": "^4.0.6",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "@storybook/storybook-deployer": "^2.7.0",
    "@types/lodash": "^4.14.118",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-modal": "^3.8.0",
    "@types/react-select": "^2.0.11",
    "@types/react-slick": "^0.23.2",
    "@types/react-table": "^6.7.21",
    "@types/react-tabs": "^2.3.0",
    "@types/storybook-addon-jsx": "^5.4.0",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.0.3",
    "@types/tinycolor2": "^1.4.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "react": "^16.6.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "redux": "^4.0.1",
    "shx": "^0.3.2",
    "slick-carousel": "^1.8.1",
    "storybook-addon-jsx": "^5.4.0",
    "styled-components": "^4.1.0",
