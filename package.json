    "react-table": "6.9.2",
    "react-tabs": "^3.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@ibm/plex": "^2.0.0",
    "@semantic-release/changelog": "^5.0.1",
